<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
          http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <servlet>
        <servlet-name>AddVehicleServlet</servlet-name>
        <servlet-class>controller.AddVehicleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddVehicleServlet</servlet-name>
        <url-pattern>/addvehicle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ViewVehiclesServlet</servlet-name>
        <servlet-class>controller.ViewVehiclesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewVehiclesServlet</servlet-name>
        <url-pattern>/viewvehicles</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateVehicleServlet</servlet-name>
        <servlet-class>controller.UpdateVehicleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateVehicleServlet</servlet-name>
        <url-pattern>/updatevehicle</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteVehicleServlet</servlet-name>
        <servlet-class>controller.DeleteVehicleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteVehicleServlet</servlet-name>
        <url-pattern>/deletevehicle</url-pattern>
    </servlet-mapping>

</web-app>
